{
	"name": "Save your eyes: 202020",
	"action":{
		"default_popup": "index.html"
	},
	"version": "0.2",
	"manifest_version": 3,
	"permissions": [
		"notifications",
		"storage",
		"scripting",
		"alarms"
	],
	"background": {
		"service_worker": "background.js"
	},
	"content_scripts":[
		{
			"matches": ["<all_urls>"],
			"js": ["timer.js"],
			"runs_at": "document_end"
		}
	]
}